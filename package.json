  "license": "MIT",
  "bugs": {
    "url": "https://github.com/philnash/mediadevices-camera-selection/issues"
  },
  "homepage": "https://github.com/philnash/mediadevices-camera-selection#readme",
  "dependencies": {
    "dotenv": "^8.2.0",
    "express": "^4.18.2",
    "twilio": "^3.60.0",
    "twilio-video": "^2.13.1"
  },
  "devDependencies": {
    "browserify": "^17.0.0",
    "concurrently": "^6.0.1",
    "copyfiles": "^2.4.1",
    "rimraf": "^3.0.2",
    "serve": "^14.1.2",
    "watchify": "^4.0.0"
  }
