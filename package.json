{
  "name": "camera-choice",
  "version": "0.1.0",
  "description": "Example code to show selecting cameras with JavaScript",
  "main": "index.js",
  "scripts": {
    "build:quickstart": "watchify video-chat/quickstart/src/index.js -o video-chat/quickstart/public/index.js",
    "clean:quickstart": "rimraf video-chat/quickstart/public/index.js",
    "start": "npm run clean:quickstart && concurrently \"npm run build:quickstart\" \"node video-chat/server\"",
    "serve": "serve ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/philnash/mediadevices-camera-selection.git"
  },
